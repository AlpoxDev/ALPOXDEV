{
   "name": "interesturl-api",
   "description": "InterestUrl API Reference",
   "version": "1.0.0",
   "private": true,
   "scripts": {
      "start": "ts-node app.ts",
      "dev": "nodemon --config nodemon.json --exec NODE_ENV=dev ts-node app.ts",
      "build": "npx tsc -p .",
      
      "test": "mocha --timeout 20000 --pretty --require ts-node/register ./tests/**/*.test.ts",
      "deploy" : "rm -rf dist && npx tsc -p . && sls deploy --verbose --aws-profile default",
      "prettier:fix": "yarn run prettier --parser typescript --single-quote --write --tab-width 4 \"./src/**/*.{ts,tsx}\""
   },
   "dependencies": {
      "@types/cors": "^2.8.6",
      "@types/express": "^4.17.2",
      "@types/jsonwebtoken": "^8.3.7",
      "aws-serverless-express": "3.3.8",
      "axios": "^0.19.2",
      "cors": "^2.8.5",
      "crypto-js": "^4.0.0",
      "date-fns": "^2.9.0",
      "express": "^4.17.1",
      "jsonwebtoken": "^8.5.1",
      "mysql2": "^2.1.0",
      "pbkdf2-password": "^1.2.1",
      "ts-node": "^8.6.2",
      "typeorm": "^0.2.22",
      "typescript": "^3.7.5",
      "uuid": "8.3.0"
   },
   "devDependencies": {
      "@types/mocha": "^5.2.7",
      "@types/supertest": "^2.0.8",
      "assert": "^2.0.0",
      "mocha": "^7.0.1",
      "nodemon": "^2.0.2",
      "prettier": "^1.19.1",
      "serverless-domain-manager": "4.2.0",
      "serverless-offline": "6.5.0",
      "supertest": "^4.0.2"
   }
}
