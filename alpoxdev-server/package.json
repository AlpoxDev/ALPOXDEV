{
   "name": "interesturl-api",
   "description": "InterestUrl API Reference",
   "version": "1.0.0",
   "private": true,
   "scripts": {
      "start": "ts-node ./src/app.ts",
      "start:prod": "node ./dist/app.js",
      "dev": "nodemon --config nodemon.json --exec ts-node src/app.ts",
      "build": "rm -rf dist && npx tsc -p .",
      "deploy:sls": "rm -rf dist && npx tsc -p . && sls deploy --verbose --aws-profile default",
      "logs:sls": "sls logs -f index --aws-profile apick",
      "test": "NODE_ENV=dev mocha --timeout 20000 --exit --pretty --require ts-node/register ./tests/User/*.test.ts",
      "prettier:1": "prettier --config ./prettier.config.js --write ./src/**/*.ts",
      "prettier:2": "prettier --config ./prettier.config.js --write ./src/**/**/.ts",
      "clear": "rm -rf dist && rm -rf .serverless"
   },
   "dependencies": {
      "aws-sdk": "2.735.0",
      "cors": "^2.8.5",
      "crypto-js": "^4.0.0",
      "dotenv": "8.2.0",
      "express": "^4.17.1",
      "express-winston": "4.0.5",
      "jsonwebtoken": "^8.5.1",
      "multer": "^1.4.2",
      "multer-s3": "^2.9.0",
      "mysql2": "^2.1.0",
      "pbkdf2-password": "^1.2.1",
      "serverless-http": "2.5.0",
      "swagger-jsdoc": "4.0.0",
      "swagger-ui-express": "4.1.4",
      "ts-node": "^8.6.2",
      "typeorm": "^0.2.25",
      "typescript": "^3.7.5",
      "uuid": "8.3.0",
      "winston": "3.3.3"
   },
   "devDependencies": {
      "@types/cors": "2.8.7",
      "@types/express": "4.17.7",
      "@types/jsonwebtoken": "8.5.0",
      "@types/mocha": "^5.2.7",
      "@types/multer": "1.4.4",
      "@types/multer-s3": "2.7.7",
      "@types/supertest": "^2.0.8",
      "@types/swagger-jsdoc": "^3.0.2",
      "@types/swagger-ui-express": "^4.1.2",
      "assert": "^2.0.0",
      "mocha": "^7.0.1",
      "nodemon": "^2.0.2",
      "prettier": "^1.19.1",
      "serverless-domain-manager": "4.2.0",
      "serverless-offline": "6.5.0",
      "serverless-webpack": "5.3.3",
      "supertest": "^4.0.2"
   }
}
